{
  "name": "qushah-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:lib": "nest build common",
    "docs:generate": "compodoc -p tsconfig.json -c apps/documentation/compodoc.config.json",
    "docs:serve": "compodoc -p tsconfig.json -n 'Qushah API Documentation' -s --hideGenerator",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "migration": "ts-node typeorm-cli.ts",
    "prepare": "husky",
    "start": "nest start",
    "start:dev:auth": "nest start --watch auth",
    "start:dev:escrow": "nest start --watch escrow",
    "start:dev:gateway": "nest start --watch gateway",
    "start:dev:media": "nest start --watch media",
    "start:dev:notification": "nest start --watch notification",
    "start:dev:payment": "nest start --watch payment",
    "start:dev:property": "nest start --watch property",
    "start:dev:user": "nest start --watch user",
    "start:dev:ms": "pnpm run start:dev:auth & pnpm run start:dev:escrow & pnpm run start:dev:media & pnpm run start:dev:notification & pnpm run start:dev:payment & pnpm run start:dev:property & pnpm run start:dev:user",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/gateway/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/gateway/test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/gateway": "^2.9.3",
    "@apollo/server": "^4.11.3",
    "@apollo/subgraph": "^2.9.3",
    "@nestjs/apollo": "^12.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.2.2",
    "@nestjs/microservices": "^10.4.15",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/yargs": "^17.0.33",
    "dotenv": "^16.4.7",
    "graphql": "^16.10.0",
    "ioredis": "^5.4.2",
    "joi": "^17.13.3",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.26",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@qushah/common(|/.*)$": "<rootDir>/libs/common/src/$1"
    }
  }
}
